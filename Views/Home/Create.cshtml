@model TEST_ASP_APP.Models.Store 
@{
    ViewBag.Title = "Добавить магазин";
}
    @*STORES*@
<form asp-action="create" asp-controller="home">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Название магазина</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Adress" class="control-label">Адрес</label>
        <input type="text" asp-for="Adress" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Opening_Time" class="control-label">Время открытия</label>
        <input type="time" asp-for="Opening_Time" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Closing_Time" class="control-label">Время закрытия</label>
        <input type="time" asp-for="Closing_Time" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Добавить" class="btn btn-default" />
    </div>
</form>